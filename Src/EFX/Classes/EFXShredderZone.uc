class EFXShredderZone expands EFXZoneInfo;

// ----------------------------------------------------------------------------
// ActorEntered()
// ----------------------------------------------------------------------------

function ActorEntered( actor Other )
{
	Super.ActorEntered(Other);

	if (!Other.IsA('PlayerPawn'))
		Other.LifeSpan = 0.01;
}

