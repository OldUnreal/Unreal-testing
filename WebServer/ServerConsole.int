<script>
var parselimit=4;
function beginrefresh()
{
	if(!document.images) return
	if (parselimit==1) window.location.replace("/ServerConsole")
	else
	{
		parselimit-=1
		setTimeout("beginrefresh()",1000)
	}
}
window.onload=beginrefresh
</script>
